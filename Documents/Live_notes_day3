DROP TABLE member
CREATE TABLE  member 
          (  id  tinyint IDENTITY(1,1)
		    ,member_name varchar(200)
			,mobile_number varchar(10)
		   )
		   

ALTER TABLE <TABLE_NAME>
ADD  <column_name> <data_type>

ALTER TABLE member
ADD  member_created datetime
 
 ALTER TABLE member
drop column member_created 



 ALTER TABLE member
ALTER  column  member_created date 



EXEC sp_rename 'schema.tablename.old_column_name'
, 'new_column_name', 'COLUMN'; 

